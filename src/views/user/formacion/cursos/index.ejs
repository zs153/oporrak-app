<!doctype html>
<html lang="en">

<head>
  <meta charset="utf-8" />
  <meta name="viewport" content="width=device-width, initial-scale=1, viewport-fit=cover" />
  <meta http-equiv="X-UA-Compatible" content="ie=edge" />
  <title>Cursos</title>
  <link rel="icon" href="/img/favicon.ico" type="image/x-icon">
  <!-- styles -->
  <link rel="stylesheet" href="/css/styles.css">
  <link rel="stylesheet" href="/css/card-stamp.min.css">
</head>

<body class="antialiased">
  <div class="wrapper">
    <%- include('../../../partials/header') %>
    <%- include('../../../partials/navbar', {opcion: 'FORMACION'}) %>
    <div class="page-header d-print-none">
      <div class="container-xl">
        <div class="row align-items-center">
          <div class="col">
            <h1 class="page-title pt-2 mb-2">
              Cursos
            </h1>
          </div>
        </div>
      </div>
    </div>
    <div class="page-wrapper">
      <div class="page-body">
        <div class="container-xl">
          <div class="row row-cards">
            <% if (datos.cursos.length===0) { %>
              <p class="text-center">
                El listado de matr√≠culas no contiene elementos
              </p>
            <% } %>
            <% datos.cursos.map(itm => { %>
            <div class="col-12">
              <div class="card">
                <div class="card-stamp">
                  <div class="card-stamp-icon bg-secondary">
                    <svg xmlns="http://www.w3.org/2000/svg" height="64" viewBox="260 0 500 900" width="64">
                      <path
                        d="M480 890q-48-37.525-104-58.263Q320 811 260 811q-40.688 0-79.922 10.214Q140.844 831.429 105 850q-21 11-40.5-1T45 814V344q0-11.355 6-21.677Q57 312 68 307q45-22.5 93.355-34.25Q209.711 261 260 261q58.5 0 113.75 15.5T480 322v478q51-32 107-48t113-16q35.486 0 69.493 5.5T840 760V285q13.5 5 27 9.75T893 307q11 5 16.5 15t5.5 21v471q0 22.755-19.75 34.628Q875.5 860.5 854 850q-36-18.5-75-28.75T700 811q-60 0-116 20.5T480 890Zm80-194V321l200-200v395L560 696Zm-155 66.5V368q-33.5-16.5-71.25-24.25T260 336q-38 0-72.75 7.5t-67.25 22v397q35-13.5 68.75-20t71-6.5q37.25 0 73.75 6.5t71.5 20Zm0 0V368v394.5Z" />
                    </svg>
                  </div>
                </div>
                <div class="card-body">
                  <h3 class="card-title"><%- itm.DESCUR %></h3>
                  <p class="text-muted">
                    <%= itm.NOTCUR %>
                  </p>
                </div>
              </div>
            </div>
            <% }) %>
          </div>
        </div>
      </div>
    </div>
    <%- include('../../../partials/footer') %>
  </div>

  <script src="/js/ayuda.min.js"></script>
</body>

</html>